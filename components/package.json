{
    "name": "@genspectrum/dashboard-components",
    "version": "1.14.1",
    "description": "GenSpectrum web components for building dashboards",
    "type": "module",
    "license": "AGPL-3.0-only",
    "main": "dist/genspectrum-components.js",
    "module": "dist/genspectrum-components.js",
    "types": "dist/genspectrum-components.d.ts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/GenSpectrum/dashboard-components.git",
        "directory": "components"
    },
    "publishConfig": {
        "access": "public"
    },
    "imports": {
        "#*": [
            "./*",
            "./*.ts",
            "./*.tsx"
        ]
    },
    "exports": {
        "./components": {
            "types": "./dist/components.d.ts",
            "import": "./dist/components.js",
            "require": "./dist/components.js"
        },
        "./util": {
            "types": "./dist/util.d.ts",
            "import": "./dist/util.js",
            "require": "./dist/util.js"
        },
        "./custom-elements.json": "./custom-elements.json",
        "./package.json": "./package.json"
    },
    "files": [
        "dist",
        "standalone-bundle",
        "src",
        "custom-elements.json",
        "package.json"
    ],
    "scripts": {
        "build": "vite --config vite.release.config.ts build && npm run generate-manifest && vite --config vite.release-standalone.config.ts build",
        "build-and-pack": "npm run build && npm pack | xargs -I {} cp {} genspectrum-dashboard-components.tgz",
        "test": "vitest --typecheck",
        "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
        "lint:eslint": "eslint 'src/**/*.{ts,tsx}'",
        "lint:lit-analyzer": "lit-analyzer",
        "generate-manifest": "npx custom-elements-manifest analyze --litelement --globs src/web-components/**",
        "generate-manifest:watch": "npm run generate-manifest -- --watch",
        "format": "prettier \"**/*.{ts,tsx,json,md,mdx,mjs,cjs}\" --write",
        "check-format": "prettier --check \"**/*.{ts,tsx,json,md,mdx,mjs,cjs}\"",
        "check-types": "tsc --noEmit",
        "check-dependencies": "depcheck",
        "storybook": "storybook dev -p 6006",
        "storybook-preact": "storybook dev --port 6007 --config-dir .storybook-preact",
        "build-storybook": "storybook build",
        "build-storybook-preact": "storybook build --config-dir .storybook-preact --output-dir storybook-static-preact",
        "test:storybook": "test-storybook",
        "test:storybook:preact": "test-storybook --config-dir .storybook-preact --url http://localhost:6007",
        "test:playwright": "playwright test",
        "test:playwright:update-snapshots": "playwright test --update-snapshots"
    },
    "customElements": "custom-elements.json",
    "keywords": [
        "web-components",
        "lit-element",
        "typescript",
        "lit"
    ],
    "dependencies": {
        "@floating-ui/dom": "^1.6.5",
        "@floating-ui/utils": "^0.2.2",
        "@lit/context": "^1.1.1",
        "@lit/reactive-element": "^2.0.4",
        "@lit/task": "^1.0.0",
        "@tanstack/table-core": "^8.21.2",
        "chart.js": "^4.4.6",
        "chartjs-chart-error-bars": "^4.4.0",
        "chartjs-chart-venn": "^4.3.0",
        "dayjs": "^1.11.10",
        "dompurify": "^3.2.4",
        "downshift": "^9.0.8",
        "flatpickr": "^4.6.13",
        "gridjs": "^6.2.0",
        "leaflet": "^1.9.4",
        "lit": "^3.1.3",
        "preact": "^10.20.1",
        "topojson-client": "^3.1.0",
        "zod": "^3.23.0"
    },
    "devDependencies": {
        "@custom-elements-manifest/analyzer": "^0.10.2",
        "@eslint/js": "^9.31.0",
        "@iconify-json/mdi": "^1.1.67",
        "@iconify-json/mdi-light": "^1.1.10",
        "@iconify/tailwind4": "^1.0.6",
        "@playwright/test": "^1.43.1",
        "@storybook/addon-actions": "^8.0.9",
        "@storybook/addon-essentials": "^8.0.9",
        "@storybook/addon-interactions": "^8.0.9",
        "@storybook/addon-links": "^8.0.9",
        "@storybook/blocks": "^8.0.10",
        "@storybook/preact": "^8.0.9",
        "@storybook/preact-vite": "^8.0.9",
        "@storybook/test": "^8.0.0",
        "@storybook/test-runner": "^0.23.0",
        "@storybook/types": "^8.0.9",
        "@storybook/web-components": "^8.0.9",
        "@storybook/web-components-vite": "^8.0.9",
        "@tailwindcss/postcss": "^4.0.9",
        "@testing-library/preact": "^3.2.4",
        "@types/geojson": "^7946.0.15",
        "@types/leaflet": "^1.9.15",
        "@types/node": "^22.0.0",
        "@types/topojson-client": "^3.1.5",
        "@types/topojson-specification": "^1.0.5",
        "@typescript-eslint/eslint-plugin": "^8.2.0",
        "@typescript-eslint/parser": "^8.2.0",
        "autoprefixer": "^10.4.19",
        "daisyui": "^5.0.0",
        "depcheck": "^1.4.7",
        "eslint": "^9.31.0",
        "eslint-plugin-import": "^2.29.1",
        "eslint-plugin-jest": "^29.12.2",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "eslint-plugin-storybook": "^0.12.0",
        "globals": "^16.3.0",
        "happy-dom": "^17.1.1",
        "http-server": "^14.1.1",
        "lit-analyzer": "^2.0.3",
        "msw": "^2.2.14",
        "postcss": "^8.4.38",
        "prettier": "^3.2.5",
        "react": "^18.3.1",
        "storybook": "^8.0.9",
        "storybook-addon-fetch-mock": "^2.0.0",
        "tailwindcss": "^4.0.9",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.36.0",
        "vite": "^6.0.3",
        "vite-plugin-dts": "4.5.0",
        "vitest": "^3.0.2"
    }
}
